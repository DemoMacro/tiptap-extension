    "office",
    "typescript",
    "editor",
    "wysiwyg",
    "rich-text"
  ],
  "author": {
    "name": "Demo Macro",
    "email": "abc@imst.xyz",
    "url": "https://imst.xyz/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DemoMacro/tiptap-extension/issues"
  },
  "homepage": "https://github.com/DemoMacro/tiptap-extension#readme",
  "dependencies": {
    "docx": "9.5.1",
    "image-meta": "0.2.2",
    "ofetch": "1.4.1"
